package com.wisdom.ncl.splitter.db.splitter;

public class DataDesc {

	private String dbName;
	private String dbtype;
	private String dbIp;
	private String dbPort;
	private String dbInitCatalog;
	private String dbuser;
	private String dbpassword;
	private String connCount;

	public DataDesc() {
	}


	public DataDesc(String dbName, String dbtype, String dbIp, String dbPort,
			String dbInitCatalog, String dbuser, String dbpassword,
			String connCount) {
		this.dbName = dbName;
		this.dbtype = dbtype;
		this.dbIp = dbIp;
		this.dbPort = dbPort;
		this.dbInitCatalog = dbInitCatalog;
		this.dbuser = dbuser;
		this.dbpassword = dbpassword;
		this.connCount = connCount;
	}


	public String getDbPort() {
	
		return dbPort;
	}


	public void setDbPort(String dbPort) {
	
		this.dbPort = dbPort;
	}


	public String getDbName() {

		return dbName;
	}

	public void setDbName(String dbName) {

		this.dbName = dbName;
	}

	public String getDbtype() {

		return dbtype;
	}

	public void setDbtype(String dbtype) {

		this.dbtype = dbtype;
	}

	public String getDbIp() {

		return dbIp;
	}

	public void setDbIp(String dbIp) {

		this.dbIp = dbIp;
	}

	public String getDbInitCatalog() {

		return dbInitCatalog;
	}

	public void setDbInitCatalog(String dbInitCatalog) {

		this.dbInitCatalog = dbInitCatalog;
	}

	public String getDbuser() {

		return dbuser;
	}

	public void setDbuser(String dbuser) {

		this.dbuser = dbuser;
	}

	public String getDbpassword() {

		return dbpassword;
	}

	public void setDbpassword(String dbpassword) {

		this.dbpassword = dbpassword;
	}

	public String getConnCount() {

		return connCount;
	}

	public void setConnCount(String connCount) {

		this.connCount = connCount;
	}

}
